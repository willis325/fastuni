    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "typescript": "^5.6.3",
    "unocss": "0.58.9",
    "unocss-applet": "0.7.8",
    "unplugin-auto-import": "^0.18.3",
    "vite": "5.2.8",
    "vite-plugin-restart": "^0.4.1",
    "vue-tsc": "^1.8.27"
  }
}
